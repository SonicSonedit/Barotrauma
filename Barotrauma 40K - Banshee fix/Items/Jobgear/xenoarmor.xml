      <GuiFrame relativesize="0.1,0.07" anchor="BottomLeft" absoluteoffset="20,20" style="ItemUI" />
      <Button text="text.bansheescream">
        <StatusEffect type="OnUse" target="This">
          <RequiredItems items="batterycell" type="Contained" targetslot="1" />
          <Sound file="%ModDir%/Items/Jobgear/Xenos/scream.ogg" type="OnUse" loop="false" range="1000" />
        </StatusEffect>
        <StatusEffect type="OnUse" target="NearbyCharacters" range="400">
          <RequiredItems items="batterycell" type="Contained" targetslot="1" />
          <Affliction identifier="bansheescream" strength="120.0" />
        </StatusEffect>
        <StatusEffect type="OnUse" target="Contained" targetslot="1" condition="-100">
          <!--<Remove/>-->
        </StatusEffect>
      </Button>
    </CustomInterface>
    <Wearable slots="Any,Head" msg="ItemMsgEquipSelect" displaycontainedstatus="true" armorvalue="50" canbepicked="true" pickkey="Select">
      <sprite name="Banshee Helmet Wearable" texture="%ModDir%/Items/Jobgear/Xenos/banshi_h.png" limb="Head" hidelimb="true" inheritlimbdepth="true" scale="0.35" hideotherwearables="false" hidewearablesoftype="hair,moustache,beard" sourcerect="0,0,215,281" origin="0.83,0.5"></sprite>
